{
  "name": "create-next-app-antd",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "generate": "prisma generate",
    "prisma-studio": "prisma studio",
    "db-seed": "prisma db seed",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "db-reset": "prisma migrate dev reset",
    "dx:next": "yarn migrate-dev db-seed && next dev",
    "dx:prisma-studio": "yarn prisma-studio",
    "dx": "run-p dx:* --print-label",
    "dev": "yarn dx:next",
    "predev": "ts-node --project ./tsconfig.node.json ./src/scripts/genAntdCss.tsx",
    "prebuild": "ts-node generate migrate && cross-env NODE_ENV=production ts-node --project ./tsconfig.node.json ./src/scripts/genAntdCss.tsx",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "yarn lint --fix",
    "postinstall": "yarn generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@ant-design/charts": "^1.4.2",
    "@ant-design/cssinjs": "^1.3.0",
    "@ant-design/static-style-extract": "~1.0.1",
    "@prisma/client": "^4.12.0",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.25.1",
    "@trpc/next": "^10.25.1",
    "@trpc/react-query": "^10.25.1",
    "@trpc/server": "^10.25.1",
    "antd": "^5.1.0",
    "next": "^13.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "superjson": "^1.7.4",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.1.4",
    "@types/react": "^18.2.6",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.3.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "^4.8.3"
  },
  "publishConfig": {
    "access": "restricted"
  }
}